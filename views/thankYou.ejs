<html>
    <head>
        <title>MEDIA Company - Thank You</title>
        <link href="css/styles.css" rel="stylesheet" type="text/css">
        <link href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.5/yeti/bootstrap.min.css" rel="stylesheet">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js
        "></script>
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        <!-- Wrap all page content here -->
        <div id="wrap">
            
            <!-- Fixed navbar -->
            <div class="navbar navbar-inverse navbar-fixed-top">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">MEDIA Company</a>
                    </div>
                    <div class="collapse navbar-collapse">
                        
                        </div><!--/.nav-collapse -->
                    </div>
                </div>
                
                <!-- Begin page content -->
                <div class="container">
                    <div class="content login-paddingTop">
                        <div class="firstArea panel panel-default">
                            <div class="row">
                                <div class="col-xs-12 padding">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-sm-6 col-md-4 col-md-offset-4">
                                                <h1 style="padding-bottom:30px" class="text-center"><%=message%></h1>
                                                <h4 style="text-align:center">We will contact you shortly.</h4>
                                                <div style="padding-bottom:20px">
                                                <a class="btn btn-info btn-md btn-block" href="/" style="width:100%;" role="button">Back to Main Page</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script>$(function () {
        $('#accordion').on('shown.bs.collapse', function (e) {
        var offset = $('.panel.panel-default > .panel-collapse.in').offset();
        if(offset) {
        $('html,body').animate({
        scrollTop: $('.panel-title a').offset().top -50
        }, 500);
        }
        });
        });</script>
        <script>
        $('.navbar .dropdown').hover(function() {
        $(this).find('.dropdown-menu').first().stop(true, true).slideDown(150);
        }, function() {
        $(this).find('.dropdown-menu').first().stop(true, true).slideUp(105)
        });</script>
    </body>
</html>